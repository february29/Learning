/* 头部 轮播图 */
.carousel-section {
  position: relative;
  padding-top: 0px;
}
.carousel-section .carousel {
    width: 100%;
    height: 350rpx;
}
.carousel-section .carousel .carousel-item {
      width: 100%;
      height: 100%;
      padding: 0px;
      overflow: hidden;
}
.carousel-section .carousel image {
      width: 100%;
      height: 100%;
}
.carousel-section .swiper-dots {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    position: absolute;
    left: 60rpx;
    bottom: 15rpx;
    width: 72rpx;
    height: 36rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}
.carousel-section .swiper-dots .num {
      width: 36rpx;
      height: 36rpx;
      border-radius: 50px;
      font-size: 24rpx;
      color: #fff;
      text-align: center;
      line-height: 36rpx;
}
.carousel-section .swiper-dots .sign {
      position: absolute;
      top: 0;
      left: 50%;
      line-height: 36rpx;
      font-size: 12rpx;
      color: #fff;
      -webkit-transform: translateX(-50%);
          -ms-transform: translateX(-50%);
              transform: translateX(-50%);
}
